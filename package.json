{
  "name": "@mingomax/landingpage",
  "version": "1.0.0",
  "description": "Starter personal site template boilerplate based on Webpack with SASS/PostCSS and babel ES6/7 support.",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run production && npm run build:web",
    "web:start": "cross-env NODE_ENV=production pm2-runtime start ecosystem.config.js --env production",
    "build:web": "babel src/server.js -d dist",
    "start": "cross-env NODE_EN=development webpack server --config config/webpack.dev.config.js --progress",
    "production": "cross-env NODE_ENV=production webpack --config config/webpack.prod.config.js",
    "lint:sass": "sass-lint -v -q",
    "lint:js": "eslint 'src/**/*,js'|| true",
    "prettify": "prettier --write 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mingomax/personalsite.git"
  },
  "keywords": [
    "webpack",
    "boilerplate",
    "landingpage",
    "html5",
    "sass",
    "javascript"
  ],
  "author": "Domingos Teruel <mingomax@dteruel.net.br>",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/mingomax/personalsite/issues"
  },
  "homepage": "https://github.com/mingomax/personalsite#readme",
  "engines": {
    "node": "^14 || >=17"
  },
  "private": false,
  "target": "web",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.7",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "file-loader": "^6.2.0",
    "html-loader": "^3.1.0",
    "html-webpack-partials-plugin": "^0.8.0",
    "html-webpack-plugin": "^5.5.0",
    "image-minimizer-webpack-plugin": "^3.2.3",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.1",
    "meow": "^10.1.2",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss": "^8.4.12",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.4.3",
    "posthtml": "^0.16.6",
    "posthtml-include": "^1.7.2",
    "posthtml-loader": "^2.0.1",
    "prettier": "^2.6.1",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "trim-newlines": "^4.0.2",
    "url-loader": "^4.1.1",
    "uuid": "^8.3.2",
    "webpack": "^5.70.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "express": "^4.17.3",
    "express-basic-auth": "^1.2.1",
    "nodemon": "^2.0.15",
    "pm2": "^5.2.0"
  }
}
